<h1>New Project</h1>

<%= form_with(model: @project, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.submit "Create Project" %>
  </div>
<% end %>

<%= link_to 'Back', projects_path %>
